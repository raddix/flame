<body>
    <amp-auto-ads type="adsense"
        data-ad-client="ca-pub-4364430965699896">
    </amp-auto-ads>
    <header class="masthead">
        <div class="container h-100">
            <div class="row h-100">
                <div class="col-lg-6" style="margin-top:17%; margin-bottom:0%; ">
                    <div class="header-content mx-auto">
                        <h1 class="cd-headline rotate-1">
                            <span>FLAMES DECIDED YOU ARE </span>
                            <span class="cd-words-wrapper" style="width: 500px">
                                <b class="is-visible">FRIENDS</b>
                                <b>L O V E R</b>
                                <b>ARRANGE</b>
                                <b>MARRIAGE</b>
                                <b>SISTERS</b>
                            </span>
                        </h1>
                    </div>
                </div>
                <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
                    <!-- Simple Ad -->
                    <ins class="adsbygoogle"
                        style="display:block"
                        data-ad-client="ca-pub-4364430965699896"
                        data-ad-slot="5663522586"
                        data-ad-format="auto"
                        data-full-width-responsive="true"></ins>
                    <script>
                    (adsbygoogle = window.adsbygoogle || []).push({});
                </script>
                <div class="col-lg-6 my-auto">

                    <div class="card my-auto" style="width: 18rem; margin-left:12%;">
                        <img class="card-img-top" <%=picSrc%> <%=altMsg%>>
                        <div class="card-body">
                            <h4 class="card-title" style="color:#880E4F;"><%=cardTitle%></h4>
                            <p class="card-text" style="color:#EC407A;"><%=cardMsg%></p>
                            <a href="https://flamesr.herokuapp.com/" class="btn btn-primary">Check More</a>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </header>

    <div class="container">

    </div>

</body>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
<script src="/js/main.js"></script>

